@model MvcCoreAWSPractica.Models.Coche

@{
    ViewData["Title"] = "Update";
}



<h1>Update</h1>

<h4>Coche</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Update">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Idcoche" class="control-label"></label>
                <input name="id" class="form-control"  value="@Model.Idcoche" readonly/>
                <span asp-validation-for="Idcoche" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Marcar" class="control-label"></label>
                <input name="marca" value="@Model.Marcar" class="form-control" />
                <span asp-validation-for="Marcar"  class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Modelo" class="control-label"></label>
                <input name="modelo" value="@Model.Modelo" class="form-control" />
                <span asp-validation-for="Modelo"  class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Conductor" class="control-label"></label>
                <input name="conductor" value="@Model.Conductor" class="form-control" />
                <span asp-validation-for="Conductor"  class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Imagen" class="control-label"></label>
                <input name="imagen" value="@Model.Imagen" class="form-control" />
                <span asp-validation-for="Imagen"  class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
